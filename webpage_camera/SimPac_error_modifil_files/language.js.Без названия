function changeLanguage(langCode){

	var langResources= getLanguageResources();
	var countryCode = ['en', 'fr', 'de', 'es', 'it', 'pt', 'ru', 'cn', 'kr'];
	var lang;
	var object;
	var txt_id;
	var attr;
	
	lang = countryCode[langCode-1];
	
	// buttons uebersetzten
    $("input[type='button']").each(function(i, elt){
		attr = $(elt).attr("name"); 
		if (typeof attr == "undefined")
		{
//				$(elt).val("no translation attrib !!!");
		}
		else
		{
			txt_id = langResources[lang][attr];
			if (typeof txt_id == "undefined")
			{
				// $(elt).val("no translation txt !!!");
			}
			else
			{
//				$(elt).val(langResources[lang][$(elt).attr("name")]);
				$(elt).val(langResources[lang][attr]);
			}
		}
    });    
    
	// submit buttons uebersetzten
    $("input[type='submit']").each(function(i, elt)
	{
		attr = $(elt).attr("name");
		if (typeof attr == "undefined")
		{
			// $(elt).val("no translation attrib !!!");
		}
		else
		{
			txt_id = langResources[lang][attr];
			if (typeof txt_id == "undefined")
			{
				// $(elt).val("no translation txt !!!");
			}
			else
			{
				$(elt).val(langResources[lang][attr]);
			}
		}
    });    
    
	// Div IDs uebersetzten
    $("[id]").each(function(i, elt){
		if ($(elt).attr("id") != "")
			$(elt).html(langResources[lang][$(elt).attr("id")]);
    });    
	
	// Tooltips uebersetzten
    $("[id='nav']").each(function(i, elt){
        $(elt).attr('title', langResources[lang][$(elt).attr("alt")]);
    });    

}


